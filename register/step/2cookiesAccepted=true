var countDownInit = Date.now() +  * 1000;

var x = setInterval(function() {
  var counter = countDownInit - Date.now() ;

  var minutes = Math.floor(counter / (1000 * 60));
  var seconds = Math.floor((counter % (1000 * 60)) / 1000);

	if (minutes <= 0 && seconds < 0) {
		minutes = 0;
		seconds = 0;
    clearInterval(x);
  }

  document.getElementById("countDown").innerHTML = "Session expires in " + ("00" + minutes).slice(-2)  + ":" + ("00" + seconds).slice(-2);
}, 1000);
</script>
